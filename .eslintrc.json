{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "space-before-function-paren": "off",
        "eol-last": "off",
        "indent": "off"
    }
}
